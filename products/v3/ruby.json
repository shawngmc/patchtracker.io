{
  "name": "Ruby",
  "id": "ruby",
  "links": [
    {
      "title": "GitHub",
      "url": "https://github.com/ruby/ruby"
    },
    {
      "title": "Homepage",
      "url": "https://www.ruby-lang.org/en/"
    }
