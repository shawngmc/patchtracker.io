{
  "name": "Ruby",
  "id": "ruby",
  "links": [
    {
      "title": "GitHub",
      "url": "https://github.com/ruby/ruby"
    },
    {
      "title": "Homepage",
      "url": "https://www.ruby-lang.org/en/"
    }
  ],
  "polling": {
    "method": "github",
    "owner": "ruby",
    "project": "ruby",
    "regexMap": [
      {
        "version": "3.x",
        "regex": {
          "test": "(v)(3)_(\\d+)_(\\d+)$",
          "parse": "$2.$3.$4"
        }
      },
      {
        "version": "2.x",
        "regex": {
          "test": "(v)(2)_(\\d+)_(\\d+)$",
          "parse": "$2.$3.$4"
        }
      }
    ]
  }
}