{
  "name": "Traefik",
  "id": "traefik",
  "links": [
    {
      "title": "GitHub",
      "url": "https://github.com/containous/traefik"
    },
    {
      "title": "Homepage",
      "url": "https://traefik.io"
    }
  ],
  "polling": {
    "method": "github",
    "owner": "containous",
    "project": "traefik",
    "regexMap": [
      {
        "version": "2.x",
        "regex": {
          "test": "(v)(2).(\\d+).(\\d+)$",
          "parse": "$2.$3.$4"
        }
      },
      {
        "version": "1.x",
        "regex": {
          "test": "(v)(1).(\\d+).(\\d+)$",
          "parse": "$2.$3.$4"
        }
      }
    ]
  }
}