{
  "name": "Passenger",
  "id": "passenger",
  "links": [
    {
      "title": "GitHub",
      "url": "https://github.com/phusion/passenger"
    },
    {
      "title": "Homepage",
      "url": "https://phusionpassenger.com/"
    }
  ],
  "polling": {
    "method": "github",
    "owner": "phusion",
    "project": "passenger",
    "regexMap": [
      {
        "version": "6.x.y",
        "regex": {
          "test": "(release-)(6).(\\d+).(\\d+)$",
          "parse": "$2.$3.$4"
        }
      },
      {
        "version": "5.x.y",
        "regex": {
          "test": "(release-)(5).(\\d+).(\\d+)$",
          "parse": "$2.$3.$4"
        }
      },
      {
        "version": "4.x.y",
        "regex": {
          "test": "(release-)(4).(\\d+).(\\d+)$",
          "parse": "$2.$3.$4"
        }
      },
      {
        "version": "3.x.y",
        "regex": {
          "test": "(release-)(3).(\\d+).(\\d+)$",
          "parse": "$2.$3.$4"
        }
      },
      {
        "version": "2.x.y",
        "regex": {
          "test": "(release-)(2).(\\d+).(\\d+)$",
          "parse": "$2.$3.$4"
        }
      },
      {
        "version": "1.x.y",
        "regex": {
          "test": "(release-)(1).(\\d+).(\\d+)$",
          "parse": "$2.$3.$4"
        }
      }
    ]
  }
}